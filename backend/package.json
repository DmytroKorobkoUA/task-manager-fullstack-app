{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend part",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "start": "node app.js",
    "migrate": "dotenv -e .env npx babel-node --require @babel/register ./node_modules/sequelize-cli/lib/sequelize db:migrate --config config/config.js",
    "seed": "dotenv -e .env npx babel-node --require @babel/register ./node_modules/sequelize-cli/lib/sequelize db:seed:all --config config/config.js",
    "test": "mocha --require @babel/register"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/node": "^7.25.0",
    "@babel/preset-env": "^7.25.2",
    "@babel/register": "^7.24.6",
    "chai": "^5.1.1",
    "chai-http": "^5.0.0",
    "dotenv-cli": "^7.4.2",
    "mocha": "^10.7.0"
  }
}
